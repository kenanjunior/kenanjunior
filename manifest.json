{
    "manifest_version": 2,

    "name": "Kenan Junior",
    "description": "Simple Chrome Extension that changes your New Tab into breathtaking Blue Universe",
    "version": "2.0",

    "browser_action": {
        "default_icon": "icon.png"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "icons": {
        "128": "icon.png",
        "64":  "icon.png",
        "32":  "icon.png",
        "16":  "icon.png"
    },

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["myScript.js"]
        }
    ],

    "permissions": [
     "tabs", 
     "https://*/*"
    ]
    

}